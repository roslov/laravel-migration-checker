{
    "name": "roslov/laravel-migration-checker",
    "type": "library",
    "description": "Up/down database migration checker for Laravel",
    "keywords": [
        "migration",
        "database",
        "up",
        "down",
        "Laravel",
        "dev"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Oleksandr Roslov",
            "email": "tr2003@gmail.com"
        }
    ],
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "roslov/migration-checker": "^0.6.0",
        "sebastian/diff": "^5.0|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "php-parallel-lint/php-console-highlighter": "^1.0",
        "php-parallel-lint/php-parallel-lint": "^1.4",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^10.5",
        "rector/rector": "^2.3",
        "roslov/psr12ext": "^14.1"
    },
    "autoload": {
        "psr-4": {
            "Roslov\\LaravelMigrationChecker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Roslov\\LaravelMigrationChecker\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true,
            "composer/package-versions-deprecated": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Roslov\\LaravelMigrationChecker\\LaravelMigrationCheckerServiceProvider"
            ]
        }
    },
    "support": {
        "source": "https://github.com/roslov/laravel-migration-checker"
    }
}
